import ejercicio1.SmashBros.*
import ejercicio2.SmashBros.*
import ejercicio3.SmashBros.*
 
test "Probar distintas espadas, que otorgan distintos potenciales ofensivos para Link" {
	var espada = new Espada(50)
	link.setArma(espada)
	assert.equals(150, link.potencialOfensivo())
	espada = new Espada(400)
	link.setArma(espada)
	assert.equals(500, link.potencialOfensivo())
} 